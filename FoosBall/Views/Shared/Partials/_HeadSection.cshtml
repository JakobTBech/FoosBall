<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
<title>
    @if (ViewBag.Environment != "Production")
    {
        @ViewBag.Environment
    }
    @ViewBag.Title
</title>
<!-- Style Sheets -->
<link href="@Url.Content("~/Content/Base.css")" rel="stylesheet" type="text/css" />
@RenderSection("Css", false)
<!-- Javascript -->
<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js"></script>
<script type="text/javascript">
    if (typeof jQuery == 'undefined') {
        document.write(unescape('%3Cscript src="@Url.Content("~/Scripts/jquery.min.js")" type="text/javascript"%3E%3C/script%3E'));
    }
</script>
<!-- Feedback plugin -->
<script type="text/javascript">
    if (!!window.innerWidth === true && window.innerWidth > 930) {
        var uvOptions = {};
        (function () {
            var uv = document.createElement('script'); uv.type = 'text/javascript'; uv.async = true;
            uv.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'widget.uservoice.com/RphctqQQTB1Xn7A577qeA.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(uv, s);
        })();
    }
</script>